{
  "name": "ai-coding-tdd-framework",
  "version": "1.0.0",
  "description": "A structured TDD framework for AI-assisted coding projects",
  "type": "module",
  "main": "dist/server/index.js",
  "scripts": {
    "dev:client": "vite",
    "dev:server": "node --loader ts-node/esm server/index.ts",
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "build:client": "tsc && vite build",
    "build:server": "tsc server/index.ts --outDir dist/server --esModuleInterop",
    "build": "npm run build:client && npm run build:server",
    "start": "node dist/server/index.js",
    "preview": "vite preview",
    "test": "vitest",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write .",
    "fetch-sounds": "node scripts/fetchSounds.js",
    "download-sounds": "node scripts/downloadFreeSounds.js",
    "generate-sounds-elevenlabs": "node scripts/fetchElevenLabsSounds.js",
    "generate-sound-effect": "node scripts/generateSoundEffect.js",
    "create-empty-sounds": "node scripts/createEmptySounds.js",
    "test-keyboard-sound": "node scripts/testKeyboardSound.js"
  },
  "keywords": [
    "ai",
    "tdd",
    "framework",
    "coding",
    "design-system"
  ],
  "author": "Joel Janov <jejanov@mac.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jejanov/codingAssitantFramework.git"
  },
  "bugs": {
    "url": "https://github.com/jejanov/codingAssitantFramework/issues"
  },
  "homepage": "https://github.com/jejanov/codingAssitantFramework#readme",
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/material": "^7.0.2",
    "axios": "^1.6.7",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "framer-motion": "^10.12.4",
    "gray-matter": "^4.0.3",
    "howler": "^2.2.4",
    "prismjs": "^1.30.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^6.10.0",
    "recharts": "^2.5.0",
    "typewriter-effect": "^2.21.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.4.3",
    "@types/express": "^4.17.17",
    "@types/howler": "^2.2.12",
    "@types/node": "^18.16.0",
    "@types/prismjs": "^1.26.5",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.14",
    "concurrently": "^9.1.2",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jsdom": "^26.0.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.7",
    "tailwindcss": "^3.3.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.2",
    "vite": "^4.3.0",
    "vitest": "^0.30.1"
  }
}
